<AudioVisual>
    <!--ensure that this value is unique, value preserved in archive when new versions are made-->
    <IdentificationNumber>{ "required": true, "unique": true, "fixed": true }</IdentificationNumber>
    <Title>{ "required": true, "summaryField": "Title" }</Title>
    <Description>{ "paragraph":true }</Description>
    <CollectionName/>

    <Subject>{ "multiple": true }
        <SubjectTag>{ "vocabulary": "PhotoSubjectTag" }</SubjectTag>
        <Entity>
            <EntityId>{ "lookup": [ "person", "place", "event" ] }</EntityId>
            <Relation>{ "vocabulary": "PhotoReferenceToRelation" }</Relation>
        </Entity>
        <Note>{ "paragraph":true }</Note>
    </Subject>

    <PhysicalObject>
        <Type>{ "vocabulary": "PhysicalFilmType" }</Type>
        <PhysicalDescription>
            <Condition>{ "vocabulary": "PhysicalPhotoCondition" }</Condition>
            ????
        </PhysicalDescription>
        <StorageLocationDescription>{ "paragraph":true }</StorageLocationDescription>
        <Copyright>
            <Holder>{ "instance": ["Person", "Group"] }</Holder>
            <CreativeCommonsLicense>{ "vocabulary": "CreativeCommonsLicense" }</CreativeCommonsLicense>
            <Notes>{ "paragraph": true }</Notes>
        </Copyright>
    </PhysicalObject>

    <DigitalObject>{ "lookup": "MediaMetadata", "multiple": true }
        <File>{ "media": "MediaMetadata }</File>
        <CreationDate>{ "":"" }</CreationDate>
        <Notes>{ "paragraph":true }</Notes>
        <TechnicalDescription>
            ????
        </TechnicalDescription>
    </DigitalObject>

    <CreationEvent>
        <!--ask: what if the creation date is not known but is a date range?-->
        <CreationDate>{ "validator": "date" }</CreationDate>
        <Creator>{ "multiple : true" }
            <Type>{ "vocabulary": "FilmCreationRole" }</Type>
            <Actor>{ "instance": [ "person", "organization" ] }</Actor>
        </Creator>
        <CreationPlace>{ "instance": "place" }</CreationPlace>
    </CreationEvent>

    <AcquisitionEvent>
        <AcquisitionType>{"vocabulary": "AcquisitionTypes"}</AcquisitionType>
        <AcquisitionSource>{ "instance": [ "person", "organization" ] }</AcquisitionSource>
        <AcquisitionDate>{"validator": "date"}</AcquisitionDate>
    </AcquisitionEvent>

    <PublicationEvent>
        <PublicationDate>{ "validator": "date" }</PublicationDate>
        <Publisher>{ "instance": [ "person", "organization" ] }</Publisher>
        <PublicationPlace>{ "instance": "place" }</PublicationPlace>
    </PublicationEvent>

</AudioVisual>